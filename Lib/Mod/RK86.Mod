MODULE [foreign] RK86; IMPORT SYSTEM;

CONST
  Bell   = 7X;
  ClrScr = 1FX;
  RusLat = 0FEX;
  NoKey  = 0FFX;

PROCEDURE- CURSOR (x, y: INT8);
PROCEDURE- GETCHAR (): CHAR;
PROCEDURE- GETKEY (): CHAR;
PROCEDURE- PRESSED (): BOOLEAN;
PROCEDURE- PUTCHAR (ch: CHAR);
PROCEDURE- PUTHEX (n: INT8);
PROCEDURE- PUTSTR (str: POINTER TO ARRAY [notag] OF CHAR);
PROCEDURE- QUIT;

END RK86.
